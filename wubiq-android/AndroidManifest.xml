<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.sf.wubiq.android"
    android:versionCode="2"
    android:versionName="1.5.0" >

    <uses-sdk android:minSdkVersion="7" android:targetSdkVersion="2.1"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN"/>
    <uses-permission android:name="android.permission.BLUETOOTH"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name">
        <activity
            android:label="@string/app_name"
            android:name=".WubiqActivity" >
            <intent-filter >
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="ConfigureBluetoothActivity"></activity>
        <activity android:name="ConfigureServerActivity"></activity>
        <activity android:name="AdvancedConfigurationActivity"></activity>
        <receiver android:label="BroadcastReceiver for boot up" android:enabled="true" android:name="PrintManagerBroadcastReceiver">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
        </receiver>
        <service android:name="PrintManagerService" android:label="@string/print_manager_service"></service>
        </application>

</manifest>